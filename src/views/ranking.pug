.rightDiv
  ul.tab
    li
      a.tablinks(href="javascript:void(0)" onclick="openRanking(event, 'daily')") Daily
    li
      a.tablinks(href="javascript:void(0)" onclick="openRanking(event, 'weekly')") Weekly
    li
      a.tablinks(href="javascript:void(0)" onclick="openRanking(event, 'alltime')") All Time
  div.tabContent#daily
    table
      thead
        tr
          th Name
          th Score
      tbody
        each round,i in dailyRanking
          tr
            td= round.username
            td= round.score
  div.tabContent#weekly
    table
      thead
        tr
          th Name
          th Score
      tbody
        each round,i in weeklyRanking
          tr
            td= round.username
            td= round.score
  div.tabContent#alltime
    h3 All Time
    p worse player all time.
