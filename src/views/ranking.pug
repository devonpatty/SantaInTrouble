.rightDiv
  ul.tab
    li
      a.dailyTab Daily
    li
      a.weeklyTab Weekly
  div.tabContent#daily
    table
      thead
        tr
          th.rank
            p Rank
          th
            p Name
          th
            p Score
      tbody
        each round,i in dailyRanking
          tr
            td.rank= i+1
            td= round.username
            td= round.score
  div.tabContent#weekly
    table
      thead
        tr
          th.rank
            p Rank
          th
            p Name
          th
            p Score
      tbody
        each round,i in weeklyRanking
          tr
            td.rank= i+1
            td= round.username
            td= round.score
